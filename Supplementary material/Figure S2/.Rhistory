#Load file
Eilat_2019<-Eilat_Full_responses_data_clean
Eilat_no_field<-subset(Eilat_2019, Temp=="27" | Temp=="29.5" | Temp=="32" | Temp=="34.5")
Hold_all<-subset(Eilat_no_field, Timepoint=="Hold")
Recovery_all<-subset(Eilat_no_field, Timepoint=="Recovery")
Control_check1<-subset(Eilat_2019, Temp=="Field" | Temp=="Start" | Temp=="27")
Control_check<-subset(Control_check1,Timepoint=="Hold")
CB.v.RSS_Control1<-aov(Chla_cell ~ Experiment*Temp,data=Control_check)
plot(CB.v.RSS_Control1)
anova(CB.v.RSS_Control1)
drop1(CB.v.RSS_Control1, test = "Chisq")
CB.v.RSS_Hold1<-lmer(Chla_cell ~ Experiment*Temp + (1|Rep),data=Hold_all)
#Statistical analyses for univariate responses
library(lme4)
library(car)
library(emmeans)
library(lmerTest)
CB.v.RSS_Hold1<-lmer(Chla_cell ~ Experiment*Temp + (1|Rep),data=Hold_all)
plot(fitted(CB.v.RSS_Hold1),residuals(CB.v.RSS_Hold1))
rvec <- seq(0,1,length=1000)
lines(rvec,predict(loess(residuals(CB.v.RSS_Hold1)~fitted(CB.v.RSS_Hold1)),newdata=rvec),col='red',lwd=2)
abline(h=0,col="gray")
# qq plot to check for normal distribution of residuals
qqnorm(resid(CB.v.RSS_Hold1)); qqline(resid(CB.v.RSS_Hold1))
# histogram of residuals to check for normal distribution of residuals
hist(resid(CB.v.RSS_Hold1))
summary(CB.v.RSS_Hold1)
Anova(CB.v.RSS_Hold1)
rand(CB.v.RSS_Hold1)
drop1(CB.v.RSS_Hold1, test = "Chisq")
CB.v.RSS_Hold2<-lmer(Chla_cell ~ Experiment+Temp + (1|Rep),data=Hold_all)
summary(CB.v.RSS_Hold2)
Anova(CB.v.RSS_Hold2)
print(emmeans(CB.v.RSS_Hold2, list(pairwise ~ Experiment)), adjust = c("tukey"))
print(emmeans(CB.v.RSS_Hold2, list(pairwise ~ Temp|Experiment)), adjust = c("tukey"))
print(emmeans(CB.v.RSS_Hold2, list(pairwise ~ Experiment|Temp)), adjust = c("tukey"))
